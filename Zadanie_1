//Pomiar czasu pÄ™tla for:
def sum_for(n=1_000_000):
    total = 0
    for i in range(1, n+1):
        total += i
        return total
        %timeit sum_for()

//Pomiar czasu wykonania list comprehension:
def sum_list_comprehension(n=1_000_000):
    return sum([i for i in range(1, n+1)])
    %timeit sum_list_comprehension()
 
//Pomiar czasu wykonania funkcji sum():
def sum_builtin(n=1_000_000):
    return sum(range(1, n+1))
 
%timeit sum_builtin()
